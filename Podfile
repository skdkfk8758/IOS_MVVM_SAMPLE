# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'IOS_MVVM_SAMPLE' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for IOS_MVVM_SAMPLE

  # RxSwift
  pod 'RxSwift'

  # RxAlamofire
  pod 'RxAlamofire'
  pod 'Alamofire'

  # SnapKit
  pod 'SnapKit'

  # RxGesture
  pod 'RxGesture'

  # ObjectMapper
  pod 'ObjectMapper'

  # SwiftJSON
  pod 'SwiftyJSON'

end
